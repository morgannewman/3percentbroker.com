{
  "name": "3percentbroker-website-remodel",
  "version": "0.0.1",
  "description": "The purpose of this remodel is to build a modern, responsive website for 3percentbroker.com in line with the basic functionality of similar real estate websites.",
  "main": "index.js",
  "scripts": {
    "start": "run-p watch-sass watch-pug & live-server ./dist",
    "watch-sass": "node-sass -rw ./src/sass -o ./dist/css --source-map ./dist/css/ --source-map-contents",
    "watch-pug": "pug -w ./src/pug -o ./dist/",
    "build": "run-s clean compile-sass build-with-parcel",
    "clean": "rm -rf dist",
    "compile-sass": "node-sass sass/main.scss dist/css/main.css",
    "build-with-parcel": "parcel build ./dist/index.html --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3percentbroker/3percentbroker.com.git"
  },
  "author": "Morgan Newman <mpnewman95@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/3percentbroker/3percentbroker.com/issues"
  },
  "homepage": "https://github.com/3percentbroker/3percentbroker.com#readme",
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-preset-env": "^1.7.0",
    "cssnano": "^4.0.5",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.7",
    "postcss-modules": "^1.3.2",
    "postcss-preset-env": "^5.3.0",
    "uglify-js": "^3.4.7"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 9"
  ],
  "dependencies": {}
}
